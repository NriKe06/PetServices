<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.doapps.petservices">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".PetServicesApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.HomeActivity" />
        <activity android:name=".Activities.SeleccionCuentaActivity" />
        <activity android:name=".Activities.CrearCuentaPersonaActivity" />
        <activity android:name=".Activities.CrearMascotaActivity" />
        <activity android:name=".Activities.CrearCuentaEmpresaActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".Activities.MessageDetailActivity"></activity>
    </application>

</manifest>